# Production Environment Variables for Vercel Deployment
# Backend API URL - Production backend deployed on Render
REACT_APP_BACKEND_URL=https://music-manager-1.onrender.com

# Build Configuration
GENERATE_SOURCEMAP=false
REACT_APP_NODE_ENV=production

# Performance Optimizations
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_CACHE_DURATION=3600

# Socket.IO Configuration
REACT_APP_SOCKET_TIMEOUT=20000
REACT_APP_SOCKET_RECONNECT_ATTEMPTS=5
